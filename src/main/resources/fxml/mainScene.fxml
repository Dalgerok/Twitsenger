<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="mainAnchorPane" prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.org.Client.MainSceneController">
   <children>
      <VBox prefHeight="800.0" prefWidth="200.0" style=":200px;">
         <children>
            <Button mnemonicParsing="false" onMouseClicked="#myProfileHandler" text="My profile" />
            <Button mnemonicParsing="false" onMouseClicked="#editMyProfileHandler" text="Edit my profile" />
            <Button mnemonicParsing="false" onMouseClicked="#myFriendsHandler" text="My friends" />
            <Button mnemonicParsing="false" onMouseClicked="#postsHandler" text="Posts" />
            <Button alignment="TOP_LEFT" mnemonicParsing="false" onMouseClicked="#logoutButtonHandler" text="Log out" />
         </children>
      </VBox>
      <Pane fx:id="mainPane" layoutX="200.0" prefHeight="800.0" prefWidth="600.0" />
   </children>
</AnchorPane>
